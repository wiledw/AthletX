                        <Font name="Montserrat Regular" size="12.0" />
                    </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
                </Button>
                <Button fx:id="Shoes" layoutX="229.0" layoutY="8.0" mnemonicParsing="false" onMouseClicked="#onShoesClicked" style="-fx-background-color: #ecedf4;" text="Shoes">
                    <font>
                        <Font name="Montserrat Regular" size="12.0" />
                    </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
                </Button>
            <Button fx:id="backButton" layoutX="929.0" layoutY="8.0" mnemonicParsing="false" onMouseClicked="#onGoBack" style="-fx-background-color: #22263E;" text="&lt;" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="forwardButton" layoutX="1001.0" layoutY="8.0" mnemonicParsing="false" onMouseClicked="#onForward" style="-fx-background-color: #22263E;" text="&gt;" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="addProduct" layoutX="793.0" layoutY="8.0" mnemonicParsing="false" onMouseClicked="#onAddProduct" style="-fx-background-color: #adb2d2; -fx-border-width: 1px 1px 1px 1px; -fx-border-color: #22263E; -fx-border-radius: 5;" text="Add Product" textFill="#22263e">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            </children>
        </Pane>
    </children>
</VBox>
