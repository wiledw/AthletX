                        <Font name="Montserrat Regular" size="12.0" />
                    </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
                </Button>
                <Button fx:id="Accessories" layoutX="118.0" layoutY="8.0" mnemonicParsing="false" onMouseClicked="#onAccessoriesClicked" style="-fx-background-color: #ecedf4;" text="Accessories">
